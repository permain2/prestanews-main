    {
      "source": "/videos/(.*)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/images",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/images/(.*)",
      "destination": "/legacy/placeholder.svg",
      "permanent": true
    },
    {
      "source": "/api/image/(.*)",
      "destination": "/legacy/placeholder.svg",
      "permanent": true
    },
    {
      "source": "/:slug/16-:id",
      "destination": "/",
      "permanent": true
