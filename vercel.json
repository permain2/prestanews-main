{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/legacy/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/news",
      "destination": "/blog",
      "permanent": true
    },
    {
      "source": "/news/(.*)",
      "destination": "/blog",
      "permanent": true
    },
    {
      "source": "/profile",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/profile/(.*)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/users",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/users/(.*)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/uploads",
      "destination": "/legacy/placeholder.svg",
      "permanent": true
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/legacy/placeholder.svg",
      "permanent": true
    },
    {
      "source": "/forums",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/forums/(.*)",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/wiki",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/wiki/(.*)",
      "destination": "/",
      "permanent": true
